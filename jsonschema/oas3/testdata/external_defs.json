{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$defs": {
    "ExternalUser": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        }
      },
      "required": ["id", "name"]
    },
    "ExternalAddress": {
      "type": "object",
      "properties": {
        "street": {
          "type": "string"
        },
        "city": {
          "type": "string"
        }
      }
    }
  }
}
